<?xml version="1.0" encoding="UTF-8" ?>
<configuration status="info">
    <appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d %p %c{1.} [%t] %m%n"/>
        </Console>

    </appenders>

    <Loggers>
        <Logger level="DEBUG" name="org.bigbase">
            <appender-ref ref="STDOUT"/>
        </Logger>
    </Loggers>

</configuration>
